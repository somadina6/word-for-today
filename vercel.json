{
  "name": "word-for-today",
  "version": 2,
  "builds": [
    { "src": "app.js", "use": "@vercel/node" },
    { "src": "/api/tweet.js", "use": "@vercel/node" }
  ],
  "crons": [
    {
      "path": "/api/tweet/tweetVOTD",
      "schedule": "0 15 * * *"
    },
    {
      "path": "/api/tweet/tweetVOTDV2",
      "schedule": "0 13 * * *"
    }
  ]
}
