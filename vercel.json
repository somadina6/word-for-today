{
  "name": "bible-verse-bot",
  "version": 2,
  "builds": [{ "src": "app.js", "use": "@vercel/node" }],
  "functions": {
    "schedule-tweet-random-verse": {
      "entryPoint": "tweetRandomVerse",
      "schedule": "cron(0 */2 * * *)"
    },
    "schedule-tweet-votd": {
      "entryPoint": "tweetVOTD",
      "schedule": "cron(0 13 * * *)"
    },
    "schedule-tweet-random-verseV2": {
      "entryPoint": "tweetRandomVerseV2",
      "schedule": "cron(0 */2 * * *)"
    },
    "schedule-tweet-votdV2": {
      "entryPoint": "tweetVOTDV2",
      "schedule": "cron(0 13 * * *)"
    }
  }
}
